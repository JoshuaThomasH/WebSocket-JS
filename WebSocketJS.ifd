						"Surprised": {
							"title": "Surprised",
							"type": "number",
							"description": "Surprised",
							"format": "double"
						}
					}
				},
				"HeadPoseEstimation": {
					"title": "Head pose",
					"type": "object",
					"description": "Head pose",
					"properties": {
						"Pitch": {
							"title": "Pitch",
							"type": "number",
							"description": "Pitch",
							"format": "double"
						},
						"Yaw": {
							"title": "Yaw",
							"type": "number",
							"description": "Yaw",
							"format": "double"
						},
						"Roll": {
							"title": "Roll",
							"type": "number",
							"description": "Roll",
							"format": "double"
						}
					}
				}
			}
