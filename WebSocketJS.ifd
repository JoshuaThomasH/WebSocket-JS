					"description": "Dwell time",
					"format": "double"
				},
				"FaceSize": {
					"title": "Face size",
					"type": "integer",
					"description": "Face size"
				},
				"MainEmotion": {
					"title": "Main emotion",
					"type": "string",
					"description": "Main emotion"
				},
				"MainEmotionConfidence": {
					"title": "Main emotion confidence",
					"type": "number",
					"description": "Main emotion confidence",
					"format": "double"
				},
				"EmotionConfidence": {
					"title": "Emotion confidence",
					"type": "object",
					"description": "Emotion confidence",
					"properties": {
						"Angry": {
							"title": "Angry",
							"type": "number",
							"description": "Angry",
							"format": "double"
						},
						"Happy": {
							"title": "Happy",
							"type": "number",
							"description": "Happy",
